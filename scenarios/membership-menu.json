{
  "viewports": [{
    "name": "desktop",
    "width": 1920,
    "height": 900
  }],
  "onBeforeScript": "login.js",
  "onReadyScript": "common/close-notifications.js",
  "scenarios": [
    {
      "label": "Membership - Dashboard",
      "url": "{url}/civicrm/member?reset=1"
    },
    {
      "label": "Membership - Find Memberships",
      "url": "{url}/civicrm/member/search?reset=1"
    },
    {
      "label": "Membership - New Membership",
      "url": "{url}/civicrm/member/add?reset=1&action=add&context=standalone"
    },
    {
      "label": "Membership - Find Memberships - Results",
      "url": "{url}/civicrm/member/search?reset=1",
      "onReadyScript": "membership/search-result.js"
    },
    {
      "label": "Membership - Batch Data Entry",
      "url": "{url}/civicrm/batch?reset=1"
    },
    {
      "label": "Membership - New Data Entry Batch",
      "url": "{url}/civicrm/batch/add?reset=1&action=add"
    },
    {
      "label": "Membership - Import Memberships",
      "url": "{url}/civicrm/member/import?reset=1"
    },
    {
      "label": "Membership Reports",
      "url": "{url}/civicrm/report/list?compid=3&reset=1"
    }
  ],
  "paths": {
    "bitmaps_reference": "./backstop_data/screenshots/membership-menu/reference",
    "bitmaps_test": "./backstop_data/screenshots/membership-menu/test",
    "engine_scripts": "./backstop_data/engine_scripts/puppet",
    "html_report": "./backstop_data/html_report/membership-menu",
    "ci_report": "./backstop_data/ci_report/membership-menu"
  },
  "engine": "puppeteer",
  "report": ["CLI", "browser"],
  "casperFlags": ["--log-level=info"],
  "asyncCaptureLimit": 1,
  "asyncCompareLimit": 50,
  "debug": false,
  "debugWindow": false
}
